<%- include ("../header.ejs") %> <%- include ("nav.ejs") %>
        <div class="main-panel">
            <div class="content-wrapper">
                <div class="row">
                    <div class="col-lg-12 grid-margin stretch-card">
                        <div class="card">
                            <div class="card-body">
                                <h4 class="card-title">
                                    <%=title%>
                                </h4>
                                <!-- <hr />
                                <p class="card-description">
                                    <a href="/applicant/add" class="btn btn-primary btn-sm btn-icon-text mr-3">
                                        Add
                                        <i class="typcn typcn-plus btn-icon-append"></i>
                                    </a>
                                </p> -->
                                <hr />
                                <% if(messages.success){%>
                                    <div class="alert alert-success" role="alert">
                                        <%= messages.success %>
                                    </div>
                                    <%}%>
                                        <% if(messages.error){%>
                                            <div class="alert alert-danger" role="alert">
                                                <%= messages.error %>
                                            </div>
                                            <%}%>
                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <table class="table table-striped project-orders-table">
                                                            <thead>
                                                                <tr>
                                                                    <th style="width: 2%">#</th>
                                                                    <th>Name</th>
                                                                    <th>Contacts</th>
                                                                    <th>Location</th>
                                                                    <th>Post</th>
                                                                    <th>Qualification</th>
                                                                    <th>Date Applied</th>
                                                                    <th></th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <% var counter=0 %>
                                                                    <% for(var i=0; i< data.length; i++) { %>

                                                                        <tr>
                                                                            <td>
                                                                                <%= ++counter %>
                                                                            </td>
                                                                            <td>
                                                                                <%= data[i].Name%>
                                                                            </td>
                                                                            <td>
                                                                                <%= data[i].Phone%> <br />
                                                                                    <%= data[i].Email%>
                                                                            </td>
                                                                            <td>
                                                                                <%= data[i].Location%>
                                                                            </td>
                                                                            <td>
                                                                                <%= data[i].Post%>
                                                                            </td>
                                                                            <td>
                                                                                <%= data[i].Qualification%>
                                                                            </td>
                                                                            <td>
                                                                                <%= data[i].DateApplied%>
                                                                            </td>

                                                                            <td>
                                                                                <div class="d-flex align-items-center">
                                                                                    <a type="button"
                                                                                        href="/applicant/edit/<%=data[i].ApplicantId%>"
                                                                                        class="btn btn-success btn-sm btn-icon-text mr-3">
                                                                                        Edit
                                                                                        <i
                                                                                            class="typcn typcn-edit btn-icon-append"></i>
                                                                                    </a>
                                                                                    <a type="button"
                                                                                        class="btn btn-danger btn-sm btn-icon-text"
                                                                                        onclick="return alert('Are you sure you want to delete THIS CAR applicant?')"
                                                                                        href="/applicant/delete/<%=data[i].ApplicantId%>">
                                                                                        Delete
                                                                                        <i
                                                                                            class="typcn typcn-delete-outline btn-icon-append"></i>
                                                                                    </a>
                                                                                </div>
                                                                            </td>
                                                                        </tr>
                                                                        <% }%>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <%- include ("../footer.ejs") %>
        </div>