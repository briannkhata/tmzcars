<%- include ("../header.ejs") %>
    <%- include ("nav.ejs") %>
        <div class="main-panel">
            <div class="content-wrapper">
                <div class="row">

                    <div class="col-lg-12 grid-margin stretch-card">
                        <div class="card">
                            <div class="card-body">
                                <h4 class="card-title">
                                    <%=title%>
                                </h4>
                                <hr>
                                <p class="card-description">
                                </p>
                                <div class="row">
                                    <div class="col-md-12">
                                        <table class="table table-striped project-orders-table">
                                            <thead>
                                                <tr>
                                                    <th>ID</th>
                                                    <th>Trans Id</th>
                                                    <th>Amount</th>
                                                    <th>Payment Date</th>
                                                    <th>Car Details</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <% var counter=0 %>
                                                    <% for(var i=0; i< data.length; i++) { %>

                                                        <tr>
                                                            <td>
                                                                <%= ++counter %>
                                                            </td>
                                                            <td>
                                                                <%= data[i].TransId%>
                                                            </td>
                                                            <td>
                                                                <%= data[i].Amount%>
                                                            </td>
                                                            <td>
                                                                <%= data[i].PaymentMethod%>
                                                            </td>
                                                            <td>
                                                                <%= data[i].DatePaid%>
                                                            </td>
                                                            <td>
                                                                <%= data[i].Car%>
                                                            </td>
                                                        </tr>
                                                        <%}%>

                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
            <%- include ("../footer.ejs") %>